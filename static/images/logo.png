  });

  </span><span><span class="hljs-keyword">const</span></span><span> translations = {
    </span><span><span class="hljs-attr">en</span></span><span>: {
      </span><span><span class="hljs-attr">hero_title</span></span><span>: </span><span><span class="hljs-string">"Welcome to Vedic Pooja Services"</span></span><span>,
